{
  "name": "hte-app",
  "version": "1.0.0",
  "description": "High-Throughput Experimentation (HTE) Design Application",
  "main": "backend/app.py",
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "cd backend && python app.py",
    "start-frontend": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "audit": "npm audit && cd frontend && npm audit",
    "audit-fix": "npm audit fix && cd frontend && npm audit fix",
    "clean": "rm -rf node_modules frontend/node_modules && npm install && cd frontend && npm install"
  },
  "dependencies": {
    "smiles-drawer": "^2.1.7",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "hte",
    "chemistry",
    "experiment",
    "react",
    "flask"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
